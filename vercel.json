{
    "functions": {
      "api/index.py": {
        "runtime": "vercel-python@3.9",
        "maxDuration": 15
      }
    },
    "routes": [
      {
        "src": "/api/index",
        "dest": "/api/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "landingpage/wsgi.py"
      }
    ]
  }
  
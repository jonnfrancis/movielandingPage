{
    "builds": [{
        "src": "landingpage/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "landingpage/wsgi.py"
        }
    ],
    "functions": {
        "movie/**/*.py": {
            "maxDuration": 15
        }
    }
}